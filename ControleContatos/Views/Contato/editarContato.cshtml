@{
    ViewData["Title"] = "Editar Contato";
}
<div class= "text-center">
   <h1>Editar Contato</h1>
</div>

<form>
  <div class="form-group">
    <label for="nome">Nome Completo</label>
    <input type="text" class="form-control" placeholder="Digite o nome do contato...">
  </div>
  <br />
  <div class="form-group">
    <label for="e-mail">Email</label>
    <input type="email" class="form-control" placeholder="Digite o e-mail do contato...">
  </div>
  <br />
  <div class="form-group">
    <label for="phone">Celular</label>
    <input type="text" class="form-control" placeholder="Digite o celular do contato...">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
  <a class="btn btn-secondary" asp-controller="Contato" asp-action="Index" role="button">Voltar</a>
</form>